{
    "files.eol": "\n",
    // Add important docker scripts to PATH
    // Enables commands such as `make all` to simply call docker
    "terminal.integrated.env.windows": {
        "ROOT": "${workspaceFolder}",
        "PATH": "${workspaceFolder}/commands;${env:Path};",
    },
    "python.pythonPath": ".env\\Scripts\\python.exe",
    "python.linting.pylintEnabled": true,
    "files.associations": {
        "*.html": "html",
        "*.oil": "c",
        "ecrobot_interface.h": "c"
    },
}